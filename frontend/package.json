{
  "name": "kkakka",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "author": "D110",
  "description": "team-project by D110",
  "main": "src/electron/main.js",
  "build": {
    "appId": "KKAKKA",
    "mac": {
      "icon": "/src/electron/icon.png"
    },
    "win": {
      "icon": "/src/electron/icon.png"
    },
    "productName": "까까",
    "copyright": "Copyright © 2024 D110",
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "src/electron/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    }
  },
  "scripts": {
    "dev": "vite",
    "elec": "electron .",
    "app": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && npm run elec\"",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "electron": "wait-on tcp:3000 && cross-env IS_DEV=true electron .",
    "electron:pack": "electron-builder --dir",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"npm run electron\"",
    "electron:builder": "electron-builder",
    "build:for:electron": "tsc --noEmit && cross-env ELECTRON=true vite build",
    "app:build": "npm run build:for:electron && npm run electron:builder"
  },
